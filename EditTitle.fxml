<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="241.0" prefWidth="497.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="iPublisher.EditTitleController">
    <children>
        <Label layoutX="178.0" layoutY="14.0" text="Edit/ Delete Title">
            <font>
                <Font name="System Bold" size="16.0" />
            </font>
        </Label>
        <Label layoutX="21.0" layoutY="83.0" text="ID" />
        <Label layoutX="21.0" layoutY="112.0" text="Name" />
        <Label layoutX="19.0" layoutY="144.0" text="Author" />
        <ComboBox fx:id="authorBox" layoutX="129.0" layoutY="140.0" prefHeight="25.0" prefWidth="217.0" />
        <TextField fx:id="idField" layoutX="129.0" layoutY="79.0" prefHeight="25.0" prefWidth="217.0" />
        <TextField fx:id="nameField" layoutX="129.0" layoutY="108.0" prefHeight="25.0" prefWidth="217.0" />
        <Button fx:id="cancelBtn" layoutX="238.0" layoutY="189.0" mnemonicParsing="false" onAction="#cancel" text="Cancel" />
        <Button fx:id="saveBtn" layoutX="318.0" layoutY="189.0" mnemonicParsing="false" onAction="#save" text="Save" />
      <Label layoutX="19.0" layoutY="54.0" text="Find Title" />
      <ComboBox fx:id="titleBox" layoutX="129.0" layoutY="50.0" onAction="#autoFill" prefHeight="25.0" prefWidth="217.0" />
      <Button fx:id="deleteBtn" layoutX="389.0" layoutY="189.0" mnemonicParsing="false" onAction="#delete" text="Delete" />
    </children>

</AnchorPane>
